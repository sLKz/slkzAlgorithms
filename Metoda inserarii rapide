#include <iostream> 

using namespace std; 

int main()
{	
    int v[50];	
    int n; 	
    
    cout << "n = ";	
    cin >> n; 	
    
    for (int i = 0; i < n; ++i)	
    {		
        cout << "v[" << i + 1 << "] = ";		
        cin >> v[i];	
    } 	
    
    cout << "\n"; 	
    
    cout << "Vectorul este: ";	
    for (int i = 0; i < n; ++i)	
    {		
        cout << v[i] << " ";	
    } 	
    
    cout << "\n"; 	
    
    for (int i = 1; i < n; ++i)
    {
        int aux = v[i];
        int st = 0, dr = i - 1;
        
        while (st <= dr)
        {
            int mijl = (st + dr) / 2;
            
            if (aux < v[mijl])
            {
                dr = mijl - 1;
            }
            else
            {
                st = mijl + 1;
            }
        }
        
        int j = i - 1;
        
        while (j >= st)
        {
            v[j + 1] = v[j];
            j--;
        }
        
        v[st] = aux;
    }
    
    cout << "Vectorul sortat este: ";	
    for (int i = 0; i < n; ++i)	
    {		
        cout << v[i] << " ";	
    }
}
