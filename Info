#include <iostream>
#include <string>
#include <vector>

int main()
{
    std::string word;
    std::vector<std::string> words;
    
    std::cout << "word = ";
    std::cin >> word;
    
    for (size_t i = 0; i < word.size(); ++i)
    {
        for (size_t j = 0; j < word.size(); ++j)
        {
            if (j + i + 1 <= word.size())
            {
                words.push_back(word.substr(j, i + 1));
            }
        }
    }
    
    for (size_t i = 0; i < words.size(); ++i)
    {
        std::cout << words[i] << " ";
    }
}
