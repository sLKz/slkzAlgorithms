#include <iostream>

using namespace std;

int main()
{
    int a[50], b[50];
    int n;
    
    cout << "n = ";
    cin >> n;
    
    for (int i = 0; i < n; ++i)
    {
        cout << "a[" << i + 1 << "] = ";
        cin >> a[i];
    }
    
    cout << "\n";
    
    cout << "Vectorul este: ";
    for (int i = 0; i < n; ++i)
    {
        cout << a[i] << " ";
    }
    
    cout << "\n";
    
    b[0] = a[0];
    
    for (int i = 1; i < n; ++i)
    {
        int j = 0;
        while (j <= i - 1 and a[i] > b[j])
        {
            ++j;
        }
        
        for (int k = i; k >= j + 1; --k)
        {
            b[k] = b[k - 1];
        }
        
        b[j] = a[i];
    }
    
    cout << "Vectorul sortat este: ";
    for (int i = 0; i < n; ++i)
    {
        cout << b[i] << " ";
    }  
}
