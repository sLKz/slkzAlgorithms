#include <iostream>

using namespace std;

int main()
{
    int a[50], b[50], k[50] = {0};
    int n;
    
    cout << "n = ";
    cin >> n;
    
    for (int i = 0; i < n; ++i)
    {
        cout << "a[" << i + 1 << "] = ";
        cin >> a[i];
    }
    
    cout << "\n";
    
    cout << "Vectorul este: ";
    for (int i = 0; i < n; ++i)
    {
        cout << a[i] << " ";
    }
    
    cout << "\n";
    
    for (int i = 0; i < n - 1; ++i)
    {
        for (int j = i + 1; j < n; ++j)
        {
            if (a[i] > a[j])
            {
                k[i]++;
            }
            else
            {
                k[j]++;
            }
        }
    }
    
    for (int i = 0; i < n; ++i)
    {
        b[k[i]] = a[i];
    }
    
    cout << "Vectorul sortat este: ";
    for (int i = 0; i < n; ++i)
    {
        cout << b[i] << " ";
    }  
}
