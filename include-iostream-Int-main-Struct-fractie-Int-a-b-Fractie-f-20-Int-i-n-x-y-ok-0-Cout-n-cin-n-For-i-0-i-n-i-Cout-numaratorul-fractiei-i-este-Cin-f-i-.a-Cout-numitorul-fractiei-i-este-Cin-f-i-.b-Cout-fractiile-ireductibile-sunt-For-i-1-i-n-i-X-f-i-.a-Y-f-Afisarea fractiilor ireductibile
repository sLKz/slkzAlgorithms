#include <iostream>
Int main () 
{Struct fractie
{Int a,b;};
Fractie f[20];
Int i,n,x,y,ok=0;
Cout<<"n=";cin>>n;
For(i=0;i<=n;i++)
{Cout<<"numaratorul fractiei"<<i<< "este";
Cin>> f[i].a;
Cout>>"numitorul fractiei"<< i << "este";
Cin>> f[i].b;}
Cout<<"fractiile ireductibile sunt";
For(i=1;i<=n;i++)
{X=f[i].a;
Y=f[i].b;
While (x!=y)
If(x>y)
X=x-y;
Else
Y=y-x;
If(x==1)
{Cout<< f[i].a<<"/"<<f[i].b<<" ";
Ok=1;
}}
If(ok==0)
Cout<<"nu exista";
Return 0;
}
