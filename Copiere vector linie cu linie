#include <iostream>

using namespace std;

int main()
{
    int vector[900], k = 0;
    int matrice[30][30], n;
    
    cout << "n = ";
    cin >> n;
    
    for (size_t i = 0; i < n * n; i++)
    {
        cout << "vector[" << i + 1 << "] = ";
        cin >> vector[i];
    }
    
    cout << "\n";
    
    for (size_t i = 0; i < n * n; i++)
    {
        cout << vector[i] << " ";
    }
    
    cout << "\n";
    
    for (size_t i = 0; i < n; i++)
    {
        for (size_t j = 0; j < n; j++)
        {
            matrice[i][j] = vector[k++];
        }
    }
    
    cout << "\n";
    
    for (size_t i = 0; i < n; i++)
    {
        for (size_t j = 0; j < n; j++)
        {
            cout << matrice[i][j] << " ";
        }
        
        cout << "\n";
    }
}
