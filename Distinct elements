#include <iostream>

int main()
{
    int v[30], n;
    int nrd = 0, d;

    std::cout << "Nr. de elem.: ";
    std::cin >> n;

    for (int i = 0; i < n; ++i)
    {
        std::cout << "v[" << i + 1 << "] = ";
        std::cin >> v[i];
    }

    for (int i = 0; i < n - 1; ++i)
    {
        for (int j = i + 1; j < n; ++j)
        {
            if (v[i] == v[j])
            {
                std::cout << "Vectorul nu e format din elemente distincte.";
                return 0;
            }
        }
    }

    std::cout << "Vectorul e format din element distincte.";
}
