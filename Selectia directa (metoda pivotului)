#include <iostream> 

using namespace std; 

int main()
{	
    int v[50];	
    int n; 	
    
    cout << "n = ";	
    cin >> n; 	
    
    for (int i = 0; i < n; ++i)	
    {		
        cout << "v[" << i + 1 << "] = ";		
        cin >> v[i];	
    } 	
    
    cout << "\n"; 	
    
    cout << "Vectorul este: ";	
    for (int i = 0; i < n; ++i)	
    {		
        cout << v[i] << " ";	
    } 	
    
    cout << "\n"; 	
    
    for (int i = 0; i < n - 1; ++i)	
    {		
        for (int j = i + 1; j < n; ++j)		
        {			
            if (v[i] > v[j])			
            {				
                int aux = v[i];				
                v[i] = v[j];				
                v[j] = aux;			
            }		
        }	
    } 	
    
    cout << "Vectorul sortat este: ";	
    for (int i = 0; i < n; ++i)	
    {		
        cout << v[i] << " ";	
    }
}
