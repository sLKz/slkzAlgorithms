#include <iostream>
using namespace std;
int main()
{
int a[20][20],n,i,j,p=1;
cout << "dimensiunea matricii";
cin >> a;
for(i=0;i<n;i++)
{
for (int j = 0; j < n; ++j)
{
cout<< "a["<<(i+1)<<"]["<<(j+1)<<"]= ";
cin >> a[i][j];

if (i > j)
{
p *= a[i][j];
}
}

cout<<"matricea citica este: "<<endl;
for(i=0;i<n;i++)
    {
	for(j=0;j<n;j++)
	cout<<a[i][j]<< " ";
	cout <<endl;
    }
cout <<"suma elem. de sub diag. princ. "<< p;
return 0;
}
